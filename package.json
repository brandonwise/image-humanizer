{
  "name": "image-humanizer",
  "version": "0.1.0",
  "description": "Transform generic AI image prompts into realistic, human-looking outputs. Detects AI-prone patterns and injects realism modifiers.",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "image-humanizer": "src/cli.js"
  },
  "scripts": {
    "start": "node src/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix"
  },
  "keywords": [
    "ai",
    "image",
    "prompt",
    "humanize",
    "midjourney",
    "stable-diffusion",
    "dall-e",
    "realistic",
    "photography"
  ],
  "author": "Brandon Wise",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonwise/image-humanizer"
  },
  "devDependencies": {
    "vitest": "^3.0.0",
    "eslint": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
